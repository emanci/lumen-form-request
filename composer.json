{
    "name": "emanci/lumen-request",
    "description": "Input request is a package for Lumen",
    "keywords": ["request-validation", "lumen-request", "lumen-request-validation"],
    "license": "MIT",
    "authors": [{
        "name": "emanci",
        "email": "zhengchaopu@gmail.com"
    }],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0",
        "laravel/lumen-framework": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Emanci\\LumenRequest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
